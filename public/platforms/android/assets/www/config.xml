ence name="LoadingDialog" value=","/> 		<preference name="LoadUrlTimeoutValue" value="20000" />		<preference name="disable-cursor" value="false" />		<gap:platform name="android" />		<feature name="http://api.phonegap.com/1.0/file"/>		<gap:plugin name="org.apache.cordova.file"  />		<gap:plugin name="org.apache.cordova.splashscreen"  />		<icon src="img/icon/hdpi.png" gap:platform="android" gap:density="xhdpi" />		<icon src="img/icon/ldpi.png" gap:platform="android" gap:density="xxhdpi" />		<icon src="img/icon/xhdpi.png" gap:platform="android" gap:density="xxxhdpi" />		<gap:splash src="img/splash/hdpi.png" gap:platform="android" gap:density="ldpi" />		<gap:splash src="img/splash/ldpi.png" gap:platform="android" gap:density="mdpi" />		<gap:splash src="img/splash/mdpi.png" gap:platform="android" gap:density="hdpi" />		<gap:splash src="img/splash/xhdpi.png" gap:platform="android" gap:density="xhdpi" />		<gap:splash src="img/splash/xxhdpi.png" gap:platform="android" gap:density="xxhdpi" />		<access origin="*" />		<gap:plugin name="com.phonegap.plugins.barcodescanner" />	</widget>