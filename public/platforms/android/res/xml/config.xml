<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.backbone-library" version="0.0.1" versionCode="" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>Backbone Library</name>
    <description>A library application that uses the Backbone and Foundation frameworks.		</description>
    <author email="dhardin07@gmail.com" href="http://cordova.io">Dustin Hardin		</author>
    <preference name="phonegap-version" value="3.5.0" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="false" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="default" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="internalOnly" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="ErrorUrl" value="" />
    <preference name="BackgroundColor" value="" />
    <preference name="KeepRunning" value="true" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="LoadingDialog" value="," />
    <preference name="LoadUrlTimeoutValue" value="20000" />
    <preference name="disable-cursor" value="false" />
    <gap:platform name="android" />
    <feature name="http://api.phonegap.com/1.0/file" />
    <gap:plugin name="org.apache.cordova.file" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <icon gap:density="xhdpi" gap:platform="android" src="img/icon/hdpi.png" />
    <icon gap:density="xxhdpi" gap:platform="android" src="img/icon/ldpi.png" />
    <icon gap:density="xxxhdpi" gap:platform="android" src="img/icon/xhdpi.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="img/splash/hdpi.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="img/splash/ldpi.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="img/splash/mdpi.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="img/splash/xhdpi.png" />
    <gap:splash gap:density="xxhdpi" gap:platform="android" src="img/splash/xxhdpi.png" />
    <access origin="*" />
    <gap:plugin name="com.phonegap.plugins.barcodescanner" />
</widget>
